<div class="col-sm-12 col-xs-12 pendingCommunity community-div" style="margin-top:5px;" id="can<%= comm._id %>">
  <div class="col-sm-1 col-xs-3" style="padding:10px;z-index:1">
    <a href="/community/communityprofile/<%= comm._id %>">
      <% if(comm.image==""){%>
        <img src="/images/nocommunityimage.png" class="cpic">
        <%}else {%>
            <img src="<%= comm.image %>" class="cpic">
        <% }%>
      </a>
    </div>
    <div class="col-sm-10 col-xs-7" style="padding-top:25px;padding-bottom:5px;overflow:scroll">
      <p style="margin:0">
        <a class="comnametxt" href="/community/communityprofile/<%= comm._id %>">
          <label class="label label-danger">Pending</label>&nbsp;&nbsp;&nbsp;<%= comm.name %>
        </a>&nbsp;&nbsp;&nbsp;<a class="comnametxt-user" style="text-decoration:none;color:black;cursor:context-menu">Members(<%= comm.membersCount %>)</a>
      </p>
    </div>
    <div class="col-sm-1 col-xs-2" style="padding:0">
      <a class="community-short-btn" onclick="cancelthisJoinRequest('<%= comm._id %>')" style="float:right">
        <label class="label label-danger" style="cursor:pointer !important;">
          <i class="fa fa-times">

          </i>
        </label>
      </a>
    </div>
  </div>
    <% }); %>
<script>
  function cancelthisJoinRequest(id)
  {
    console.log("ID IS "+id);
                 $.ajax({
                  type: "POST",
                 url: "/cancelJoinRequest",
                 data: JSON.stringify({commid : id}),
                 contentType: 'application/json',
                 success: function()
                 {
                    var child= document.getElementById("can"+id);
                    child.parentNode.removeChild(child);
                 }
                });
            }
  </script>

     