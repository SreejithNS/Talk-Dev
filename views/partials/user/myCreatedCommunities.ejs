
       
       <% commPopulated.myCreatedCommunities.forEach(function(comm){ %>
       <div class="col-sm-12 col-xs-12 myCommunity community-div" style="margin-top:5px;" id="can<%= comm._id %>">
          <div class="col-sm-1 col-xs-3" style="padding:10px;z-index:1">
            <a href="/community/discussion/<%= comm._id %>">
              <% if(comm.image==""){%>
                <img src="/images/nocommunityimage.png" class="cpic">
                <%}else {%>
                    <img src="<%= comm.image %>" class="cpic">
                <% }%>
            </a>
          </div>
          <div class="col-sm-10 col-xs-7" style="padding-top:25px;padding-bottom:5px;overflow:scroll">
            <p style="margin:0">
              <a class="comnametxt" href="/community/discussion/<%= comm._id %>">
                <%= comm.name %>
              </a>&nbsp;&nbsp;&nbsp;<a class="comnametxt-user" href="/community/manageCommunity/<%= comm._id %>">Request(<%= comm.membersCount %>)</a>
            </p>
        </div>
        <div class="col-sm-1 col-xs-2" style="padding:0">
                <a class="community-short-btn" href="/community/manageCommunity/<%= comm._id %>" style="float:rignt">
                    <label class="label label-success" style="cursor:pointer !important;">
                        <i class="fa fa-cogs"></i>
                    </label>
                </a>
        </div>
</div>
       <% }); %>

        